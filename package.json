{
  "name": "@adso-sass/flags",
  "version": "1.0.0",
  "description": "SCSS flags collection",
  "repository": {
    "type": "git",
    "url": "git@github.com:adam-sokolowski/flags.git"
  },
  "keywords": [
    "sass",
    "skeleton"
  ],
  "main": "dist/_index.scss",
  "files": [
    "dist"
  ],
  "scripts": {
    "sass:lint": "stylelint './**/*.scss' --syntax scss",
    "sass:compile": "sass src/flags/_index.scss dist/index.css",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "reset:githooks": "rm -rf ./.git/hooks/* && rm -rf node_modules/husky && yarn install --check-files",
    "storybook:compile": "start-storybook -p 6006"
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "yarn sass:lint"
      ],
      "pre-push": [
        "yarn sass:compile"
      ]
    }
  },
  "author": "Adam Soko≈Çowski <adam.sokolowski@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@storybook/html": "^5.3.18",
    "babel-loader": "^8.1.0",
    "husky": "4.2.3",
    "path": "0.12.7",
    "sass": "1.26.3",
    "sass-loader": "8.0.2",
    "standard-version": "7.1.0",
    "stylelint": "13.3.1",
    "stylelint-order": "4.0.0",
    "stylelint-scss": "3.16.1"
  },
  "dependencies": {}
}
